{
  "expo": {
    "name": "DcDrivers",
    "owner": "rasheedshina",
    "slug": "Dc-Drivers",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#45130F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.dcdriver.App"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#45130F"
      },
      "package": "app.dcdriver.App",
      "versionCode": 3,
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
