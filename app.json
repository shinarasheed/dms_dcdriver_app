{
  "expo": {
    "name": "Dcdriver",
    "owner": "rasheedshina",
    "slug": "dc-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#45130F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#45130F"
      },
      "package": "app.dcdriver.App",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
